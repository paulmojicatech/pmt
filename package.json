    "marvintherapy-api": "cd apps/marvintherapy-api/marvintherapy-api && dotnet run",
    "docker-deploy-fantalytic-public": "nx build fantalytic-public --prod && docker build -t paulmojicatech/fantalytic-public-image -f fantalytic-public-dockerfile . && docker push paulmojicatech/fantalytic-public-image",
